package Day_7;

import java.util.List;

public class RequestParser {
	String request;
	List<String> requestList;
	List<String> firstLineParsed;
	public RequestParser(String request){
		this.request = request;
	}
	
	public void requestParser(){
		String[] requestParsed = request.split("\n");
		requestList.add(requestParsed[0]);
		requestList.add(requestParsed[1]);
		
		String[] firstLineParsedtoString = requestList.get(0).split(" ");
		
		for(String el : firstLineParsedtoString){
			firstLineParsed.add(el);
		}
		
		requestList.get(1).split(" ");
		
	}
	
	public void createRequest(){
//		Map<String, String> map = requestList.get(requestList.size() - 1);
		Request newRequest = new Request(
				requestList.get(0), requestList.get(1), requestList.get(2), requestList.get(3), null);
		
	}
}
