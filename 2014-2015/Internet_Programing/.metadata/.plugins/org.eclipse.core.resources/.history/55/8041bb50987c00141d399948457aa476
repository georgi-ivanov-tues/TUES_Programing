package Day_7;

import java.util.List;
import java.util.Map;

public class RequestParser {
	
	public static Request parse(List<String> request){
		String[] firstLineParsed = request.get(0).split(" ");
		
		String[] secondLine = request.get(1).split(": ");
		map.put(secondLine[0], secondLine[1]);
	
		Request newRequest = new Request(
				firstLineParsed[0], firstLineParsed[1], firstLineParsed[2], map, null);		
	}
}
